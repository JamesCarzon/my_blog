set.seed(1)
data.frame(statistic = rnorm(100000)) %>%
filter(abs(statistic) < 2.5) %>%
ggplot() +
geom_density(aes(x = statistic),
fill = "green",
alpha = 0.5) +
geom_vline(xintercept = qnorm(c(0.025, 0.975))) +
geom_hline(yintercept = 0) +
geom_rect(data = data.frame(xmin = -Inf,
xmax = qnorm(0.025),
ymin = -Inf,
ymax = Inf),
aes(xmin = xmin,
xmax = xmax,
ymin = ymin,
ymax = ymax),
fill = "red",
alpha = 0.25) +
geom_rect(data = data.frame(xmin = qnorm(0.975),
xmax = Inf,
ymin = -Inf,
ymax = Inf),
aes(xmin = xmin,
xmax = xmax,
ymin = ymin,
ymax = ymax),
fill = "red",
alpha = 0.25) +
geom_rect(data = data.frame(xmin = qnorm(0.025),
xmax = qnorm(0.975),
ymin = -Inf,
ymax = Inf),
aes(xmin = xmin,
xmax = xmax,
ymin = ymin,
ymax = ymax),
fill = "blue",
alpha = 0.25)
